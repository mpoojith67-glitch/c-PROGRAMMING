#include <stdio.h>
int main()
{
    int r = 5;

    for (int i = 0; i < r; i++)
    {
        for (int j = 0; j < 2 * i; j++)
        {
            printf(" ");
        }

        // Second inner loop printing stars *
        for (int k = 0; k < 2 * (r - i) - 1; k++)
        {
            printf("%d", k+1);
        }
        printf("\n");
    }
}
